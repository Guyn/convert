{
	"name": "@guyn/convert",
	"version": "1.5.0",
	"description": "Convert Colors into multiple files",
	"main": "dist/cli.js",
	"module": "dist/cli.module.js",
	"browser": "dist/cli.browser.js",
	"bin": {
		"convert": "dist/cli.js"
	},
	"scripts": {
		"test": "npm run build && jest --verbose && npm run test:success && npm run test:procreate",
		"test:happy": "sh test/happy.sh",
		"test:success": "sh test/test.sh",
		"test:errors": "sh test/errors.sh",
		"test:procreate": "sh test/procreate.sh",
		"test:npx": "npx guyner --src test/source/valid/sourc1.json --dest tmp/npx-test --template scss",
		"lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
		"lint:fix": "eslint --ext .js,.ts --ignore-path .gitignore . --fix",
		"build": "tsc",
		"build:dev": "tsc -w"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/guyn/convert.git"
	},
	"keywords": [
		"Convert",
		"Colors",
		"Files"
	],
	"author": "Sil van Diepen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/guyn/convert/issues"
	},
	"homepage": "https://github.com/guyn/convert#readme",
	"dependencies": {
		"ansi-256-colors": "^1.1.0",
		"cli-block": "^1.1.6",
		"ejs": "^3.0.1",
		"jszip": "^3.2.2",
		"kleur": "^3.0.3",
		"mono-str-width": "^1.0.0",
		"str-convert": "0.0.1",
		"window-size": "^1.1.1",
		"yargs": "^15.1.0"
	},
	"devDependencies": {
		"@types/jest": "^25.1.4",
		"@types/node": "^13.11.1",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-prettier": "^3.1.2",
		"jest": "^25.2.4",
		"prettier": "^2.0.3",
		"ts-jest": "^25.3.0",
		"tslint": "^6.1.1",
		"typescript": "^3.8.3"
	},
	"directories": {
		"lib": "lib",
		"test": "test"
	}
}
