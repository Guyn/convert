{
	"name": "@guyn/convert",
	"version": "1.4.6",
	"description": "Convert Colors into multiple files",
	"main": "cli.js",
	"bin": {
		"convert": "dist/cli.js"
	},
	"scripts": {
		"test": "npm run build && jest --verbose && npm run test:success && npm run test:procreate",
		"test:success": "npm run build && sh test/test.sh",
		"test:errors": "npm run build && sh test/errors.sh",
		"test:procreate": "npm run build && sh test/procreate.sh",
		"test:npx": "npx guyner --src test/source/valid/sourc1.json --dest tmp/npx-test --template scss",
		"build": "tsc",
		"build:package": "npm run build && rollup -c"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/silvandiepen/guyner.git"
	},
	"keywords": [
		"Convert",
		"Colors",
		"Files"
	],
	"author": "Sil van Diepen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/silvandiepen/guyner/issues"
	},
	"homepage": "https://github.com/silvandiepen/guyner#readme",
	"dependencies": {
		"ansi-256-colors": "^1.1.0",
		"cli-block": "^1.1.2",
		"ejs": "^3.0.1",
		"jszip": "^3.2.2",
		"kleur": "^3.0.3",
		"mono-str-width": "^1.0.0",
		"window-size": "^1.1.1",
		"yargs": "^15.1.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^11.0.2",
		"@rollup/plugin-json": "^4.0.2",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"@rollup/plugin-url": "^4.0.2",
		"@svgr/rollup": "^5.2.0",
		"@types/jest": "^25.1.4",
		"jest": "^25.2.4",
		"rollup": "^1.32.0",
		"rollup-plugin-peer-deps-external": "^2.2.2",
		"rollup-plugin-preserve-shebangs": "^0.1.2",
		"rollup-plugin-terser": "^5.2.0",
		"ts-jest": "^25.3.0",
		"tslint": "^6.1.1",
		"typescript": "^3.8.3"
	},
	"directories": {
		"lib": "lib",
		"test": "test"
	}
}
